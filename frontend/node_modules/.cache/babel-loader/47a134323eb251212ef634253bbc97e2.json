EAAA;AAPA,OADA;AAUA,MAAA,eAAA,EAAA,KAVA;AAWA,MAAA,eAAA,EAAA;AAXA,KAAA;AAaA,GApBA;AAqBA,EAAA,OAAA,EAAA;AACA,IAAA,cADA,4BACA;AACA,UAAA,QAAA,GAAA,QAAA,CAAA,cAAA,wBAAA,KAAA,SAAA,CAAA,EAAA,EAAA;AACA,UAAA,SAAA,GAAA,QAAA,CAAA,cAAA,WAAA,KAAA,SAAA,CAAA,EAAA,EAAA;;AACA,UAAA,KAAA,eAAA,EAAA;AAAA;AACA,QAAA,QAAA,CAAA,SAAA,GAAA,WAAA;AACA,QAAA,SAAA,CAAA,KAAA,CAAA,eAAA,GAAA,OAAA;AAEA,QAAA,KAAA,CAAA,MAAA,CAAA,mBAAA,EAAA,IAAA;AACA,OALA,MAMA;AAAA;AACA,QAAA,QAAA,CAAA,SAAA,GAAA,SAAA;AACA,QAAA,SAAA,CAAA,KAAA,CAAA,eAAA,GAAA,kBAAA;AAEA,QAAA,KAAA,CAAA,MAAA,CAAA,kBAAA,EAAA,IAAA;AACA;;AACA,WAAA,eAAA,GAAA,CAAA,KAAA,eAAA;AACA,KAjBA;AAkBA,IAAA,2BAlBA,yCAkBA;AACA,UAAA,KAAA,KAAA,CAAA,WAAA,CAAA,MAAA,GAAA,CAAA,EAAA,OAAA,iBAAA;AACA,aAAA,gBAAA;AACA,KArBA;AAsBA,IAAA,aAtBA,2BAsBA;AACA,WAAA,eAAA,GAAA,IAAA;AACA,KAxBA;AAyBA,IAAA,aAzBA,2BAyBA;AACA,WAAA,eAAA,GAAA,KAAA;AACA,KA3BA;AA4BA,IAAA,UA5BA,wBA4BA;AACA,MAAA,QAAA,CAAA,cAAA,CAAA,KAAA,KAAA,CAAA,EAAA,EAAA,MAAA;AACA;AA9BA,GArBA;AAqDA,EAAA,OArDA,qBAqDA;AACA,SAAA,KAAA,CAAA,EAAA,GAAA,KAAA,SAAA,CAAA,EAAA;;AACA,QAAA,KAAA,KAAA,EAAA;AACA,UAAA,aAAA,GAAA,KAAA,SAAA,CAAA,SAAA;AACA,UAAA,YAAA,GAAA,EAAA;;AAFA,iDAGA,aAHA;AAAA;;AAAA;AAGA,4DAAA;AAAA,cAAA,QAAA;AACA,UAAA,YAAA,IAAA,QAAA;AACA;AALA;AAAA;AAAA;AAAA;AAAA;;AAMA,WAAA,KAAA,CAAA,SAAA,GAAA,YAAA;AACA,KAPA,MAQA,KAAA,KAAA,CAAA,MAAA,GAAA,KAAA,SAAA,CAAA,MAAA;;AACA,SAAA,KAAA,CAAA,OAAA,GAAA,KAAA,SAAA,CAAA,KAAA;AACA,SAAA,KAAA,CAAA,QAAA,GAAA,KAAA,SAAA,CAAA,QAAA,CAAA,WAAA,EAAA;AACA,SAAA,KAAA,CAAA,IAAA,GAAA,KAAA,SAAA,CAAA,IAAA,CAAA,KAAA,CAAA,GAAA,EAAA,CAAA,CAAA;AACA,SAAA,KAAA,CAAA,WAAA,GAAA,KAAA,SAAA,CAAA,WAAA;AACA;AApEA,CAAA","sourcesContent":["<template>\n  <div  :id=\"`${emailInfo.id}`\" class=\"email-card\" \n        @mouseover=\"showQuickMenu()\" @mouseleave=\"hideQuickMenu()\">\n\t\t<div :id=\"`select-email-${emailInfo.id}`\" class=\"unChecked\" @click=\"toggleCheckBox()\"></div>\n    <div id=\"sender-recivers\" class=\"content\">{{inbox ? email.receivers: email.sender}}</div>\n    <div id=\"subject\" class=\"content\">{{email.subject}}</div>\n    <div id=\"priority\" :class=\"['content', `${email.priority}`]\">\n      <label for=\"priority-select\" class=\"priority-label\">{{email.priority}}</label> &#9679;\n    </div>\n    <div id=\"date\" class=\"content\">{{email.date}}</div>\n    <div id=\"attachments\" class=\"content\">\n      <img src=\"../../assets/compose/attach.png\" width=\"20px\" :class=\"showHideAttachemntIndicator()\">\n    </div>\n    <div :class=\"['quick-menu', activeQuickMenu ? 'visible': 'hidden', chechBoxChecked ? 'mouse-on': 'mouse-leave']\">\n      <div class=\"menu-btn\">\n        <img src=\"../../assets/general/moveFolder.png\" width=\"18px\">\n      </div>\n      <div class=\"menu-btn\">\n        <img src=\"../../assets/sideMenu/trash-gray.png\" width=\"18px\">\n      </div>\n    </div>\n  </div>\n  \n</template>\n\n<script>\nimport store from '../../store'\nexport default {\n  name: 'emailCard',\n  props: {\n    inbox: Boolean,\n    emailInfo: Object,\n  },\n  data(){\n    return{\n      email: {\n        id: String,\n        sender: String,\n        receivers: \"\",\n        subject: String,\n        priority: String,\n        date: String,\n        attachments: Array,\n      },\n      activeQuickMenu: false,\n      chechBoxChecked: false,\n    }\n  },\n  methods: {\n    toggleCheckBox(){\n      const checkBox = document.getElementById(`select-email-${this.emailInfo.id}`);\n      const emailCard = document.getElementById(`${this.emailInfo.id}`);\n      if(this.chechBoxChecked){ //un select Email\n        checkBox.className = \"unChecked\";\n        emailCard.style.backgroundColor = \"white\";\n\n        store.commit('removeSelectEmail', this);\n      }\n      else {//select Email\n        checkBox.className = \"checked\";\n        emailCard.style.backgroundColor = \"rgb(224,224,224)\"\n\n        store.commit('addSelectedEmail', this);\n      }\n      this.chechBoxChecked = !this.chechBoxChecked;\n    },\n    showHideAttachemntIndicator(){\n      if(this.email.attachments.length > 0) return \"has-attachments\";\n      return \"no-attachments\";\n    },\n    showQuickMenu(){\n      this.activeQuickMenu = true;\n    },\n    hideQuickMenu(){\n      this.activeQuickMenu = false;\n    },\n    removeSelf(){\n      document.getElementById(this.email.id).remove();\n    },\n  },\n  mounted(){\n    this.email.id = this.emailInfo.id;\n    if(this.inbox) {\n       const receiversList = this.emailInfo.receivers;\n       let recieversStr = \"\";\n       for(const reciever of receiversList){\n         recieversStr += reciever;\n       }\n       this.email.receivers = recieversStr;\n    }\n    else this.email.sender = this.emailInfo.sender;\n    this.email.subject = this.emailInfo.title;\n    this.email.priority = this.emailInfo.priority.toLowerCase();\n    this.email.date = this.emailInfo.time.split(' ')[0];\n    this.email.attachments = this.emailInfo.attachments;\n  },\n}\n</script>\n\n<style scoped>\n.email-card{\n  width: 100%;\n  height: 2.5rem;\n  display: grid;\n  grid-template-columns: 5% 20% 35% 15% 15% 10%;\n  grid-template-rows: 2rem;\n  justify-items: start;\n  align-items: center;\n  justify-content: center;\n  align-content: center;\n  border-bottom: 1px solid rgb(224, 224, 224);\n  background-color: white;\n}\n.email-card:hover{\n  box-shadow: 1px 0px 5px 1px rgb(224, 224, 224);\n  transform: scale(1);\n  transition: tranafrom 0.2s ease, box-shadow 0.2s ease;\n}\n.urgent{\n  color: #fc033d;\n}\n.high{\n  color: #fc6703;\n}\n.normal{\n  color: #ffd900;\n}\n.low{\n  color: #1da355;\n}\n.content{\n  height: 2rem;\n  width: 100%;\n  text-align: center;\n  vertical-align: middle;\n  line-height: 2rem;\n  font-size: 15px;\n  grid-row: 1 / 2;\n}\n\n#sender-recivers{\n  overflow-x: auto;\n  font-weight: bold;\n  grid-column: 2 / 3;\n}\n#subject{\n  font-weight: bold;\n  grid-column: 3 / 4;\n}\n#priority{\n  grid-column: 4 / 5;\n}\n#date{\n  grid-column: 5 / 6;\n}\n#attachments{\n  grid-column: 6 / 7;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n.no-attachments{\n  visibility: hidden;\n  display: none;\n}\n.has-attachments{\n  visibility: visible;\n  display: block;\n}\n\n.checked{\n  grid-column: 1/2;\n  width: 0.9rem;\n  height: 0.9rem;\n  margin-left: 0.5rem;\n  border: 2px solid #3474E0;\n  border-radius: 0.3rem;\n  background-color: #3474E0;\n  transition: 0.2s ease;\n}\n.unChecked{\n  grid-column: 1/2;\n  width: 0.9rem;\n  height: 0.9rem;\n  border: 2px solid rgb(224, 224, 224);\n  border-radius: 0.3rem;\n  margin-left: 0.5rem;\n  transition: 0.2s ease;\n}\n\n.quick-menu{\n  position: absolute;\n  height: 2.5rem;\n  width: 5rem;\n  right: 0rem;\n  padding-right: 1.5rem;\n  grid-column: 5 / 7;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  align-items: center;\n  z-index: 2;\n}\n.mouse-on{\n  background-color: rgb(224, 224, 224);\n}\n.mouse-leave{\n  background-color: white;\n}\n.menu-btn{\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  height: 2rem;\n  width: 2rem;\n  border-radius: 50%;\n}\n.menu-btn:hover{\n  background-color: rgb(224, 224, 224, 0.6);\n}\n\n.visible{\n  visibility: visible;\n  display: flex;\n}\n.hidden{\n  visibility: hidden;\n  display: flex;\n}\n\n.priority-label{\n  font-weight: bold;\n}\n</style>"],"sourceRoot":"src/components/compose"}]}