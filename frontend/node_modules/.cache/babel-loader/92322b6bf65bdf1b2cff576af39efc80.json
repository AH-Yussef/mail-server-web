tEmail: (state, email) => {\n      const emailIndex = state.selectedEmails.indexOf(email);\n      state.selectedEmails.splice(emailIndex, 1);\n    },\n    clearSelecteEmails: state => state.selectedEmails.length = 0,\n  };\n    \n  export default{\n      state,\n      getters,\n      actions,\n      mutations,\n  };"]}]}